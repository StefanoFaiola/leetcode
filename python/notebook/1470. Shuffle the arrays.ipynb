  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [],
   "source": [
    "nums = [2,5,1,3,4,7]\n",
    "n = 3"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "metadata": {},
   "outputs": [],
   "source": [
    "def shuffle(nums, n):\n",
    "    l = []\n",
    "    for i in range(0,n):\n",
    "        l.append(nums[i])\n",
    "        l.append(nums[i+n])\n",
    "    return l\n",
    "\n"
   ]
  },
  {
